#include "corne_ulp_ball.dtsi"

&xiao_i2c {
    status = "okay";
    clock-frequency = <I2C_BITRATE_STANDARD>;
    trackball: trackball@a {
        compatible = "palette,az1uball";
        reg = <0x0A>;
		status = "okay";
    };
};

&trackball_input_listener {
    status = "okay";
    device = <&trackball>;
};
